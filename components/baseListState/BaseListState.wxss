.state-box {
  padding: 30rpx 0;
  font-size: 30rpx;
  color: #666;
  width: 100%;
  text-align: center;
}
.state-box .loading {
  display: flex;
  align-items: center;
  justify-content: center;
}
.state-box  .loading .loading-icon {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx; 
}
.state-box .no-more-data{
  color: #999999; 
}
.state-box  .no-data {
  margin: 0;
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #999999; 
}
